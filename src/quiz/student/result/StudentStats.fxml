<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="700.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="quiz.student.result.StudentStatsController">
   <children>
      <Pane fx:id="Statistics" prefHeight="700.0" prefWidth="700.0">
         <children>
            <Label alignment="CENTER" layoutX="204.0" layoutY="88.0" prefHeight="45.0" prefWidth="252.0" text="Student Statistics" textFill="#3c11c9">
               <font>
                  <Font size="30.0" />
               </font>
            </Label>
            <ChoiceBox fx:id="period" layoutX="330.0" layoutY="370.0" onMouseClicked="#onPeriodSelected" prefHeight="27.0" prefWidth="162.0" />
            <Label layoutX="108.0" layoutY="370.0" prefHeight="27.0" prefWidth="202.0" text="Select Period:">
               <font>
                  <Font size="18.0" />
               </font></Label>
            <Label layoutX="108.0" layoutY="273.0" prefHeight="27.0" prefWidth="150.0" text="Select Report:">
               <font>
                  <Font size="18.0" />
               </font></Label>
            <ChoiceBox fx:id="report" layoutX="330.0" layoutY="273.0" onMouseClicked="#reportSelect" prefHeight="27.0" prefWidth="162.0" />
            <Button fx:id="viewStats" layoutX="473.0" layoutY="586.0" mnemonicParsing="false" onAction="#viewStats" prefHeight="50.0" prefWidth="120.0" text="View Stats" />
            <Button layoutX="56.0" layoutY="586.0" mnemonicParsing="false" onAction="#goBack" prefHeight="50.0" prefWidth="120.0" text="Back" />
            <Label layoutX="108.0" layoutY="201.0" text="Select Student:" />
            <ChoiceBox fx:id="studDrop" layoutX="330.0" onMouseClicked="#studentSelect" layoutY="196.0" prefHeight="31.0" prefWidth="162.0" />
         </children>
      </Pane>
   </children>
</AnchorPane>
