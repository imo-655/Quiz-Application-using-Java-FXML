<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="700.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="quiz.student.view.StartTestController">
   <children>
      <Pane prefHeight="700.0" prefWidth="700.0">
         <children>
            <Label alignment="CENTER" layoutX="235.0" layoutY="88.0" prefHeight="13.0" prefWidth="190.0" text="Take Test" textFill="#3c11c9">
               <font>
                  <Font size="30.0" />
               </font>
            </Label>
            <ChoiceBox fx:id="numOfQuestions" layoutX="330.0" layoutY="370.0" onMouseClicked="#onQuestionSelected" prefHeight="27.0" prefWidth="162.0" />
            <Label layoutX="108.0" layoutY="370.0" prefHeight="27.0" prefWidth="202.0" text="Number of Questions :">
               <font>
                  <Font size="18.0" />
               </font></Label>
            <Label layoutX="108.0" layoutY="273.0" prefHeight="27.0" prefWidth="150.0" text="Difficulty Level:">
               <font>
                  <Font size="18.0" />
               </font></Label>
            <ChoiceBox fx:id="diffLevel" layoutX="330.0" layoutY="273.0" onMouseClicked="#onDiffSelected" prefHeight="27.0" prefWidth="162.0" />
            <Button layoutX="473.0" layoutY="586.0" mnemonicParsing="false" onAction="#onBeginTest" prefHeight="50.0" prefWidth="120.0" text="Begin Test" />
            <Button layoutX="56.0" layoutY="586.0" mnemonicParsing="false" onAction="#goBackToStudentDashboard" prefHeight="50.0" prefWidth="120.0" text="Back" />
         </children>
      </Pane>
   </children>
</AnchorPane>
